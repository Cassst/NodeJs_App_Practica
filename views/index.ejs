<!DOCTYPE html>
<html lang="es">

 <%- include('./partials/head.ejs') %>
 
  <body>
    <%- include('./partials/nav.ejs') %>
    <div class="container">
      <ul class="collection with-header">
        <% if (items.length > 0) { %> <% items.forEach((item) => { %>
        <li class="collection-item">
          <div><%= item.name%> $<%= item.price %><a href="#!" class="secondary-content"><i class="material-icons">delete</i><i class="material-icons">edit</i></a></div>
        </li>
        <% }); %> <%}else {%>
        <li>No hay items actualmente</li>
        <%}%>
      </ul>
    </div>
  </body>
</html>
